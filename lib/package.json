{
  "name": "vindur",
  "description": "A Compile time CSS in JS library",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:update-snapshots": "vitest run --u ",
    "lint": "pnpm tsc && pnpm eslint",
    "tsc": "tsc -p tsconfig.prod.json",
    "eslint": "CI=true eslint --color src/ --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "pnpm lint && pnpm build:no-test",
    "build:no-test": "tsup --minify --dts",
    "npm-publish": "pnpm publish --access=public",
    "pre-publish": "../scripts/check-if-is-sync.sh && pnpm build"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": "github:lucasols/vindur",
  "author": "Lucas Santos",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/generator": "^7.28.0",
    "@ls-stack/extended-lint": "^0.3.1",
    "@ls-stack/utils": "^3.17.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.7",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitest/ui": "^2.1.3",
    "babel-plugin-react-compiler": "^19.0.0-beta-8a03594-20241020",
    "eslint": "^9.13.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.6.2",
    "tsm": "^2.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^4.5.5",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "babel-plugin-react-compiler": "*",
    "vite": ">=4.0.0||>=5.0.0"
  }
}
