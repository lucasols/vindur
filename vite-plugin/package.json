{
  "name": "@vindur-css/vite-plugin",
  "version": "0.2.0",
  "description": "Vite plugin for Vindur CSS-in-JS library",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "pnpm tsc && pnpm eslint",
    "tsc": "tsc --noEmit",
    "eslint": "eslint src --max-warnings 0 --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "npm-publish": "pnpm publish --access=public",
    "pre-publish": "../scripts/check-if-is-sync.sh && pnpm build"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "vindur": "*"
  },
  "devDependencies": {
    "@ls-stack/utils": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "rollup": "^4.46.1",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vindur": "workspace:*"
  },
  "keywords": [
    "vite",
    "plugin",
    "css",
    "css-in-js",
    "vindur"
  ],
  "author": "Lucas Santos",
  "license": "MIT"
}
