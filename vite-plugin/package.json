{
  "name": "@vindur/vite-plugin",
  "version": "0.1.0",
  "description": "Vite plugin for Vindur CSS-in-JS library",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit && eslint src --ext .ts",
    "tsc": "tsc --noEmit",
    "eslint": "eslint src --ext .ts"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "catalog:",
    "@ls-stack/utils": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "vindur": "workspace:*"
  },
  "keywords": [
    "vite",
    "plugin",
    "css",
    "css-in-js",
    "vindur"
  ],
  "author": "Vindur Team",
  "license": "MIT"
}
