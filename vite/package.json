{
  "name": "@vindur/vite",
  "version": "0.1.0",
  "description": "Vite plugin for Vindur CSS-in-JS library",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit && eslint src --ext .ts",
    "tsc": "tsc --noEmit",
    "eslint": "eslint src --ext .ts"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@ls-stack/utils": "^3.17.1",
    "@types/node": "^22.0.0",
    "eslint": "^9.32.0",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "vite",
    "plugin",
    "css",
    "css-in-js",
    "vindur"
  ],
  "author": "Vindur Team",
  "license": "MIT"
}